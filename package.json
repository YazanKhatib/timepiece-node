{
  "name": "backend-node",
  "version": "1.0.0",
  "description": "timepiece backend nodejs application",
  "main": "app.js",
  "repository": "https://gitlab.com/chrono24/backend-node.git",
  "author": "<yazankhatib97@gmail.com>",
  "license": "MIT",
  "private": true,
  "scripts": {
    "build": "tsc -p .",
    "dev": "nodemon --exec ts-node src/app.ts",
    "start": "ts-node dist/src/app.js",
    "migrate": "knex --knexfile=src/knexfile.ts migrate:latest",
    "rollback": "knex --knexfile=src/knexfile.ts migrate:rollback"
  },
  "_moduleAliases": {
    "graph/*": "src/graph/*",
    "graph": "src/graph/index.ts",
    "models/*": "src/models/*",
    "models": "src/models/index.ts",
    "controllers/*": "src/controllers/*",
    "controllers": "src/controllers/index.ts",
    "services/*": "src/services/*",
    "services": "src/services/index.ts",
    "middlewares/*": "src/middlewares/*",
    "middlewares": "src/middlewares/index.ts"
  },
  "devDependencies": {
    "@types/multer": "^1.4.5",
    "eslint": "^7.14.0",
    "eslint-config-prettier": "^6.15.0",
    "eslint-plugin-prettier": "^3.1.4",
    "prettier": "^2.2.0",
    "typescript": "^4.0.5"
  },
  "dependencies": {
    "@types/bcrypt": "^3.0.0",
    "@types/express": "^4.17.8",
    "@types/graphql": "^14.5.0",
    "@types/jsonwebtoken": "^8.5.0",
    "@types/node": "^14.14.6",
    "@types/nodemailer": "^6.4.0",
    "@types/randomatic": "^3.1.0",
    "@types/validator": "^13.1.2",
    "@typescript-eslint/eslint-plugin": "^4.8.1",
    "@typescript-eslint/parser": "^4.8.1",
    "apollo-server": "^2.19.0",
    "bcrypt": "^5.0.0",
    "class-validator": "^0.12.2",
    "cors": "^2.8.5",
    "crypto": "^1.0.1",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "firebase-admin": "^9.4.2",
    "graphql": "^15.4.0",
    "graphql-upload": "^11.0.0",
    "jsonwebtoken": "^8.5.1",
    "knex": "^0.21.12",
    "module-alias": "^2.2.2",
    "multer": "^1.4.2",
    "nodemailer": "^6.4.16",
    "nodemon": "^2.0.6",
    "objection": "^2.2.3",
    "otp-generator": "^2.0.0",
    "pg": "^8.4.2",
    "randomatic": "^3.1.1",
    "reflect-metadata": "^0.1.13",
    "ts-node": "^9.0.0",
    "type-graphql": "^1.1.1",
    "winston": "^3.3.3"
  }
}
